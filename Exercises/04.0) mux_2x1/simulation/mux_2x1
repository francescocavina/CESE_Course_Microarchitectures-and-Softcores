#! /usr/local/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/local/lib/ivl/system.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/local/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/local/lib/ivl/va_math.vpi";
S_0x55ce3091cad0 .scope module, "mux_2x1_tb" "mux_2x1_tb" 2 4;
 .timescale 0 0;
v0x55ce309300b0_0 .var "a_tb", 0 0;
v0x55ce30930170_0 .var "b_tb", 0 0;
v0x55ce30930240_0 .net "c_tb", 0 0, v0x55ce3092fe70_0;  1 drivers
v0x55ce30930340_0 .var "sel_tb", 0 0;
S_0x55ce3091cc60 .scope module, "DUT" "mux_2x1" 2 32, 3 4 0, S_0x55ce3091cad0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /INPUT 1 "sel";
    .port_info 3 /OUTPUT 1 "c";
v0x55ce308e2cf0_0 .net "a", 0 0, v0x55ce309300b0_0;  1 drivers
v0x55ce3092fdb0_0 .net "b", 0 0, v0x55ce30930170_0;  1 drivers
v0x55ce3092fe70_0 .var "c", 0 0;
v0x55ce3092ff40_0 .net "sel", 0 0, v0x55ce30930340_0;  1 drivers
E_0x55ce308e31b0 .event anyedge, v0x55ce3092ff40_0, v0x55ce3092fdb0_0, v0x55ce308e2cf0_0;
    .scope S_0x55ce3091cc60;
T_0 ;
    %wait E_0x55ce308e31b0;
    %load/vec4 v0x55ce3092ff40_0;
    %cmpi/e 0, 0, 1;
    %jmp/0xz  T_0.0, 4;
    %load/vec4 v0x55ce308e2cf0_0;
    %store/vec4 v0x55ce3092fe70_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55ce3092fdb0_0;
    %store/vec4 v0x55ce3092fe70_0, 0, 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x55ce3091cad0;
T_1 ;
    %vpi_call 2 11 "$dumpfile", "mux_2x1_tb.vcd" {0 0 0};
    %vpi_call 2 12 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55ce3091cad0 {0 0 0};
    %delay 1000, 0;
    %vpi_call 2 13 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x55ce3091cad0;
T_2 ;
    %vpi_call 2 17 "$display", $time, " << Starting the Simulation >>" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55ce309300b0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55ce30930170_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55ce30930340_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55ce30930340_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55ce30930170_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55ce30930340_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55ce309300b0_0, 0, 1;
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
